.clearfix::after{
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.bannerw{
  width: 750rpx;
  height: 750rpx;
  position: relative;
}
.bans{
  width: 750rpx;
  height: 750rpx;
}
.bans .img{
  width: 750rpx;
  height: 750rpx;
}
.bans .img image{
  display: block;
  width: 750rpx;
  height: 750rpx;
}
.pageban{
  position: absolute;
  z-index: 500;
  width: 110rpx;
  height: 50rpx;
  background: #4e4d4e;
  border-radius: 30rpx;
  overflow: hidden;
  font-size: 26rpx;
  color: #fffefe;
  text-align: center;
  line-height: 50rpx;
  bottom: 30rpx;
  right: 20rpx;
}
/**nature标题***/
.nature{
  height: 250rpx;
  padding: 0 15rpx;
  background: #fff;
  border-bottom: 2rpx solid #eeeeee;
}
.nature .titw{
  width: 720rpx;
  height: 130rpx;
}
.nature .titw .tit{
  width: 540rpx;
  height: 105rpx;
  font-size: 28rpx;
  color: #000000;
  line-height: 50rpx;
  overflow: hidden;
  margin-right: 35rpx;
}
.xian2{
  height: 75rpx;
  width: 2rpx;
  background: #eeeeee;
}
.nature .titw .scw{
  width: 140rpx;
  height: 105rpx;
  overflow: hidden;
}
.nature .titw .scw .sc .ico{
  width: 35rpx;
  height: 35rpx;
  margin: 0 auto;
}
.nature .titw .scw .sc .ico image{
  display: block;
  width: 35rpx;
  height: 35rpx;
}
.nature .titw .scw .sc .text{
  font-size: 24rpx;
  color: #000000;
  line-height: 30rpx;
  margin-top: 20rpx;
  text-align: center;
}
.dyprice{
  height: 95rpx;
  line-height: 95rpx;
  font-size: 32rpx;
  color: #0cc8a5;
  font-weight: bold;
}
.supaddr{
  padding: 0 15rpx;
  background: #ffffff;
  height: 120rpx;
  line-height: 120rpx;
}
.supaddr .lab{
  line-height: 120rpx;
  font-size: 30rpx;
  color: #1e1e1e;
}
.supaddr .addr{
  font-size: 24rpx;
  color: #5a5a5a;
  line-height: 120rpx;
}
/***联系详情***/
.relation{
  height: 255rpx;
  background: #fff;
  padding: 0 15rpx;
}
.relation .tit{
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 2rpx solid #eeeeee;
  font-size: 30rpx;
  color: #1e1e1e;
}
.relation .info{
  padding: 20rpx 0;
  height: 115rpx;
}
.relation .info .linkman{
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #5a5a5a;
  margin: 20rpx 0;
}
.relation .info .ico{
  width: 30rpx;
  height: 30rpx;
  display: block;
  margin-right: 15rpx;
}
.relation .info .ico image{
  display: block;
  width: 30rpx;
  height: 30rpx;
}
.relation .info .tel{
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #5a5a5a;
  margin: 20rpx 0;
}
/***promise**/
.promise{
  padding: 30rpx 15rpx;
  background: #fff;
}
.promise .item{
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #2b2b2b;
  margin-right: 20rpx;
}
.promise .item .ico{
  width: 30rpx;
  height: 30rpx;
  display: block;
  margin-right: 15rpx;
}
.promise .item .ico image{
  display: block;
  width: 30rpx;
  height: 30rpx;
}
/****描述*/
.describe{
  padding: 0 15rpx;
  background: #fff;
}
.describe .tit{
  height: 130rpx;
  line-height: 130rpx;
  font-size: 28rpx;
  color: #000000;
}
.describe .text{
  font-size: 26rpx;
  color: #000000;
  line-height: 35rpx;
}
/**回到顶部**/
.fixedRight{
  width: 90rpx;
  height: 210rpx;
  position: fixed;
  right: 25rpx;
  bottom: 40rpx;
  z-index: 500;
}
.fixedRight .circular{
  width: 95rppx;
  height: 95rpx;
  border-radius: 50%;
  overflow: hidden;
}
.fixedRight .circular image{
  width: 95rpx;
  height: 95rpx;
  display: block;
}
/***底部**/
.fixedfoot{
  position: fixed;
  width: 750rpx;
  height: 135rpx;
  bottom: 0;
  left: 0;
  background: #eeeeee;
  z-index: 500;
}
.fixedfoot .foot{
  height: 85rpx;
  width: 750rpx;
}
.fixedfoot .lfoot{
  padding: 0 20rpx;
}
.fixedfoot .foot .section{
  width: 100rpx;
  display: block;
  padding: 0;
  margin: 0;
  border: 0;
  outline: 0;
  background: rgba(0, 0, 0, 0);
  overflow: hidden;
  height: 85rpx;
}
.fixedfoot .foot .section .img{
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin: 0 auto;
}
.fixedfoot .foot .section .img image{
  display: block;
  width: 40rpx;
  height: 40rpx;
}
.fixedfoot .foot .section .text{
  font-size: 24rpx;
  color: #434343;
  text-align: center;
  line-height: 25rpx;
  margin-top: 15rpx;
}
button::after{
  border:0;
}
.fixedfoot .foot .btnwr{
  width: 470rpx;
  height: 75rpx;
}
.fixedfoot .foot .btnwr .ljcg{
   width: 470rpx;
  height: 75rpx;
  text-align: center;
  line-height: 75rpx;
  font-size: 24rpx;
