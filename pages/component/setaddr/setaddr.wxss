/* pages/component/setaddr/setaddr.wxss */
.fl{
  float:left;
}
.fr{
  float:right;
}
.clearfix::after{
  display: block;
  height: 0;
  content: ".";
  visibility: hidden;
  clear:both;
}
.h-20{
  height: 20rpx;
  background: #eeeeee;
}
.h-25{
  height: 25rpx;
}
.h-100{
  height: 100rpx;
}
.setaddrw{
  width: 750rpx;
}
.setaddrw .item{
  height: 245rpx;
  background: #fff;
}
.setaddrw .item .addr{
  font-size: 30rpx;
  color: #2b2b2b;
  height: 30rpx;
  line-height: 30rpx;
  padding: 30rpx 20rpx;
}
.setaddrw .item .name{
  font-size: 24rpx;
  color: #5e5e5e;
  line-height: 25rpx;
  padding: 0 20rpx;
}
.setaddrw .item .name text{
  margin-left: 25rpx;
}
.setaddrw .item .handle{
  height: 35rpx;
  border-top: 1rpx solid #eeeeee;
  overflow: hidden;
  padding: 25rpx 20rpx;
  font-size: 24rpx;
  color: #2b2b2b;
  line-height: 35rpx;
}
.setaddrw .item .handle .default{
  width: 155rpx;
  height: 35rpx;
}
.setaddrw .item .handle .default .icon{
  width: 31rpx;
  height: 31rpx;
  display: block;
  border-radius: 50%;
  border:2rpx solid #a5a5a5;
  margin-right: 10rpx;
  overflow: hidden;
}
.setaddrw .item .handle .default .checked{
  width: 35rpx;
  height: 35rpx;
  display: block;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 10rpx;
}
.setaddrw .item .handle .default .checked image{
  display: block;
  width: 35rpx;
  height: 35rpx;
}
.setaddrw .item .handle .delete .icon{
   width: 35rpx;
    height: 35rpx;
    display: block;
    margin-right: 10rpx;
}
.setaddrw .item .handle .delete .icon image{
  display: block;
  width: 35rpx;
  height: 35rpx;
}
.setaddrw .item .handle .editor{
  margin-right: 35rpx;
}
.setaddrw .item .handle .editor .icon{
   width: 35rpx;
    height: 35rpx;
    display: block;
    margin-right: 20rpx;
}
.setaddrw .item .handle .editor .icon image{
  display: block;
  width: 35rpx;
  height: 35rpx;
}
.newaddaddr{
  width: 750rpx;
  height: 100rpx;
  background: #eeeeee;
  position: fixed;
  bottom: 0;
}
.newaddaddr .shdz{
  width: 245rpx;
  height: 40rpx;
  font-size: 30rpx;
  color: #2b2b2b;
  margin: 35rpx auto;
}
.newaddaddr .shdz .ico{
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin-right: 15rpx;
}
.newaddaddr .shdz .ico image{
  display: block;
  width: 40rpx;
  height: 40rpx;
}